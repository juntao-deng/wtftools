<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.6"?>
<plugin>

<!--建工程扩展点-->
 <extension
         point="org.eclipse.jdt.core.classpathContainerInitializer">
      <classpathContainerInitializer
            class="com.thimda.plugin.proj.WEBClasspathInitializer"
            id="com.thimda.rsd.library.container"/>
   </extension>
    
   
    <extension
         id="com.thimda.plugin.proj.ModuleBuilder"
         name="ModuleBuilder"
         point="org.eclipse.core.resources.builders">
      <builder hasNature="true">
         <run class="com.thimda.plugin.proj.ModuleBuilder"/>
      </builder>
   </extension>

	<extension
         id="com.thimda.project.nature"
         name="rsd project nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run class="com.thimda.plugin.proj.WEBProject"/>
      </runtime>
      <requires-nature id="org.eclipse.jdt.core.javanature"/>
      <builder id="com.thimda.plugin.proj.ModuleBuilder"/> 
    </extension>
   
 

  <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="RSD工程"
            id="com.thimda.newWizards"/>
      <wizard
            category="com.thimda.newWizards"
            class="com.thimda.plugin.wizards.NewWebModuleProjectWizard"
            finalPerspective="org.eclipse.jdt.ui.JavaPerspective"
            icon="icons/project.gif"
            id="com.thimda.plugin.wizards.NewWebModuleProjectWizard"
            name="RSD工程向导"
            preferredPerspectives="org.eclipse.jdt.ui.JavaPerspective"
            project="true"/>
   </extension>
   
     
      
      
     <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IResource"
            id="com.thimda.MDEToolSet">
         <menu
               label="Rsd Tools"
               id="com.thimda.MDETools">
            <separator name="group0"/>
         </menu>
         <filter
               value="com.thimda.project.nature"
               name="projectNature"/>
         <action
               class="com.thimda.plugin.proj.UpdateRSDClasspathAction"
               id="com.thimda.plugin.proj.UpdateRSDClasspathAction"
               label="Update RSD ClassPath"
               menubarPath="com.thimda.MDETools/group0"/>
       	</objectContribution>
    </extension>
   
   
   
   
   
   
 
</plugin>
